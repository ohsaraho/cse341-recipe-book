{
    "recipeName": "Homemade Stuffed Shells",
    "ingredients": [
        {
            "quantity": "6 8oz cans",
            "name": "tomato sauce",
            "type": "Vegetable/Fruit"
        },
        {
            "quantity": "2 Tbsp",
            "name": "cane sugar",
            "type": "Drinks"
        },
        {
            "quantity": "1 cup",
            "name": "water",
            "type": "Drinks"
        },
        {
            "quantity": "2 tsp",
            "name": "parsley flakes",
            "type": "Seasoning"
        },
        {
            "quantity": "1 3/4 tsp",
            "name": "basil flakes",
            "type": "Seasoning"
        },
        {
            "quantity": "1 3/4 tsp",
            "name": "oregano flakes",
            "type": "Seasoning"
        },
        {
            "quantity": "1 3/4 tsp",
            "name": "onion granules",
            "type": "Seasoning"
        },
        {
            "quantity": "1 3/4 tsp",
            "name": "garlic granules",
            "type": "Seasoning"
        },
        {
            "quantity": "1 tsp",
            "name": "salt",
            "type": "Seasoning"
        },
        {
            "quantity": "1 pck",
            "name": "Barilla Jumbo Shells",
            "type": "Pasta"
        },
        {
            "quantity": "1 cup",
            "name": "cheddar cheese",
            "type": "Cheese"
        },
        {
            "quantity": "1 cup",
            "name": "mozzarella cheese",
            "type": "Cheese"
        }
    ],
    "instructions": [
        "Add all tomato sauce and seasoning into a medium pot and whisk together until combined.",
        "Cook for 5 minutes on medium heat.",
        "Cook noodles as directed on the box.",
        "Drain pasta and rinse of with cold water.",
        "Grade both cheeses and put aside",
        "9X13 casserole dish with a thin layer of sauce on the bottom",
        "Stuff shells with 1/2 Tbsp of cheese",
        "Pour sauce on top and bake for 20 minutes on 350F",
        "Cool down then serve"
    ],
