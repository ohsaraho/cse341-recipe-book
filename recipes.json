    "recipeName": "Hash Browns",
    "ingredients": [
        {
            "quantity": "1 lb",
            "name": "potatoes",
            "type": "Vegetable"
        },
        {
            "quantity": "1/4 tsp",
            "name": "onion granules",
            "type": "Seasoning"
        },
        {
            "quantity": "1/4 tsp",
            "name": "garlic granules",
            "type": "Seasoning"
        },
        {
            "quantity": "1/4 tsp",
            "name": "salt",
            "type": "Seasoning"
        },
        {
            "quantity": "1/4 tsp",
            "name": "black pepper",
            "type": "Seasoning"
        }
    ],
    "instructions": [
        "Wash potatoes then wrap the potatoes indvidually in tin foil. Bake potatoes on 400F for about an hour or until fork inserts smooth. ",
        "Cool the potatoes over night unwrapped.",
        "Grade potatoes and fry in a pan or grill with oil.",
        "Add seasoning to taste and cook until light brown then serve while hot."
    ],
    "prepareTime": "30 minutes",
